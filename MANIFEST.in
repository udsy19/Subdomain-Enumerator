# Ultra-Robust Subdomain Enumerator - Manifest for packaging

# Include documentation
include README.md
include TECHNICAL_README.md
include DEPLOYMENT.md
include requirements.txt

# Include scripts
include main.py
include tui_main.py
include main_tui.py

# Include wordlists
recursive-include wordlists *.txt

# Include configuration examples
include *.json.example

# Exclude development files
exclude .DS_Store
exclude *.pyc
global-exclude __pycache__

# Exclude build artifacts
prune dist/
prune build/
prune *.egg-info/